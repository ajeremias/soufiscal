<p id="notice"><%= notice %></p>

<h1>Complaint Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @complaint_addresses.each do |complaint_address| %>
      <tr>
        <td><%= complaint_address.nome %></td>
        <td><%= complaint_address.state %></td>
        <td><%= link_to 'Show', complaint_address %></td>
        <td><%= link_to 'Edit', edit_complaint_address_path(complaint_address) %></td>
        <td><%= link_to 'Destroy', complaint_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Complaint Address', new_complaint_address_path %>
